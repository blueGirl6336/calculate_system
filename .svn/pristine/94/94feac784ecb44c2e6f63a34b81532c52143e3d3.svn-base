#main-info
{
	position:absolute;
	margin-left:260px;
	background-color: #fff;
	width:1248px;
	height:100%;
}
#title1
{
	margin-left:50px;
	margin-right: 50px;
	height:80px;
	line-height: 80px;
	border-bottom: 1px solid;
	border-color: rgb(238,239,243);
	font-size: 36px;
	color:rgb(49,51,40);
	vertical-align: text-bottom;
}
#title1 span
{
	position: relative;
	top: 15px;
}
#main-table
{
	position:absolute;
	margin-top: 20px;
	margin-left: 50px;
	width:1148px;
	height:78%;
	border:1px solid;
	border-color: rgb(238,239,243);
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
#see-rank
{
	height:40px;
	line-height: 40px;
	background-color: rgb(245,245,245);
	font-size:16px;
	vertical-align: middle;
}
#see-rank span 
{
	position: relative;
	left:15px;
}

