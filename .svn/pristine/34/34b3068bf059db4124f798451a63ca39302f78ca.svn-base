#appealMyPrize
{
	width: 500px;
	height: 320px; 
	position: fixed;
	margin-top: 0px;
	margin-left:300px;
	z-index: 99999; 
	display: none;
	background-color: rgb(245,245,245);
}
#systemTitle
{
	height:40px;
	text-align: center;
	background-color: rgb(51,122,183);
	color: #fff;
	font-size: 20px;
	border:2px solid grey;
}
#tip
{
	height:30px;
	line-height: 30px;
	text-align: center;
	font-size: 16px;
}
#text
{
	width:400px;
	height:150px;
	position: absolute;
	margin-left:50px;
}
#ok
{
	width: 80px;
	height: 30px;
	position: absolute;
	left: 150px;
	margin-top: 180px;
}
#cancel
{
	width: 80px;
	height: 30px;
	position: absolute;
	left: 250px;
	margin-top: 180px;
}